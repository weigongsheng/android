<?xml version="1.0" encoding="utf-8"?>
<com.hiuzhong.qiqiu.view.ListFootView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/listViewFooter"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:orientation="vertical" >


    <ProgressBar
        android:id="@+id/listFooterProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:layout_gravity="center"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/listFooterTextTip"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center"
        android:gravity="center"
       android:clickable="true"
        android:textColor="@color/fond_load_finish_color"
        android:textSize="@dimen/font_load_finish_size"
        android:text="@string/text_load_finish" 
        
        />
	
</com.hiuzhong.qiqiu.view.ListFootView>
